if(!missing(psinhat)){
psi <- reshape2::melt(psinhat$psi, id.vars = c("listpair"), value.name = "psi")%>% separate(variable, c("model", "method"))
sigma2 <- reshape2::melt(psinhat$sigma2, id.vars = c("listpair"), value.name = "sigma2")%>% separate(variable, c("model", "method"))
n <- reshape2::melt(psinhat$n, id.vars = c("listpair"), value.name = "n")%>% separate(variable, c("model", "method"))
varn <- reshape2::melt(psinhat$varn, id.vars = c("listpair"), value.name = "varn")%>% separate(variable, c("model", "method"))
N <- psinhat$N
cin.l <- reshape2::melt(psinhat$cin.l, id.vars = c("listpair"), value.name = "cin.l")%>% separate(variable, c("model", "method"))
cin.u <- reshape2::melt(psinhat$cin.u, id.vars = c("listpair"), value.name = "cin.u")%>% separate(variable, c("model", "method"))
result<- merge(psi, sigma2, by = c("listpair", "model", "method")) %>%
merge(n, by = c("listpair", "model", "method")) %>%
merge(varn, by = c("listpair", "model", "method")) %>%
merge(cin.l, by = c("listpair", "model", "method")) %>%
merge(cin.u, by = c("listpair", "model", "method"))
ggplot(result, aes(x = model, color = method)) +
#geom_line(aes(y = n, linetype = method)) +
geom_point(aes(y = n), position=position_dodge(0.25)) +
geom_errorbar(aes(ymin = cin.l, ymax = cin.u), width=.2, position=position_dodge(0.25)) +
scale_x_continuous(name = "conditional variable (number of observations)", breaks = c(N$condvar), labels = paste(N$condvar, " (", N$N, ')', sep = '')) +
theme_bw()
